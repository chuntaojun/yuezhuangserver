<?xml version="1.0" encoding="utf-8"?>
<!-- 指定Hiberante3映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yueserver.enity.User" table="User" schema="YueZhuang">
        <id name="userno" type="java.lang.Integer" column="UsrNo">
            <generator class="identity" />
        </id>
        <property name="usrname" type="java.lang.String" column="UsrName" />
        <property name="userimage" type="java.lang.String" column="UsrImage" />
        <property name="usertime" type="java.util.Date" column="UsrTime" />
        <property name="userintro" type="java.lang.String" column="UsrIntro" />
        <property name="usrstatus" type="java.lang.Integer" column="" />
    </class>
</hibernate-mapping>